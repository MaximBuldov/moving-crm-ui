{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "comma-dangle": [
      "warn",
      "never"
    ],
    "curly": [
      "warn",
      "all"
    ],
    "camelcase": "off",
    "no-console": "warn",
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "import/export": "off",
    "import/named": "off",
    "import/default": "off",
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "unknown"
        ],
        "newlines-between": "always"
      }
    ],
    "linebreak-style": "off",
    "max-len": "off",
    "no-duplicate-imports": "error",
    "no-mixed-operators": "error",
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 1
      }
    ],
    "no-undef": "off",
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "quote-props": [
      "warn",
      "as-needed"
    ],
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      "warn",
      "always"
    ]
  }
}